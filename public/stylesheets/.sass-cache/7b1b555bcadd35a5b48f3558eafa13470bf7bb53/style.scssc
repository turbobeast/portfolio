@media screen and ( max-width: 780px ) {
	header h1 {
		font-size: 42px;
		background-size: 42px 30px;
		padding-left: 56px;
		background-position: 4px 8px;
	}

	header h2 {
		font-size: 20px;
	}

	nav {
		font-size: 32px;
		li {
			padding-right: 12px;
		}
	}

	.stage {

		padding-top: 180px;

		.chosen , .ghost {
			margin-bottom: 12px;
			h3 {
				padding: 8px;
				font-size: 42px;
				padding-left: 16px;
			}

			.description {
				padding: 8px 16px;
			}


			.tags {
				padding-left: 16px;
				line-height: 0;
				margin-bottom: 24px;
				margin-top: 8px;
			}

			.tags li {
				display: inline-block;
				font-size: 22px;
				padding-right: 14px;

			}

			address {
				font-size: 16px;
				margin-bottom: 4px;
			}


			.clink {
				font-size: 16px;
				margin-bottom: 4px;
			}

			
		}
	}

}



@media screen and ( max-width: 660px ) {


	header h1 {
		font-size: 34px;
		background-size: 26px 16px;
		padding-left: 42px;
		background-position: 8px 3px;

	}

	header h2 {
		font-size: 18px;
	}

	nav {
		font-size: 28px;
		margin-top: 6px;
		li {
			padding-right: 8px;

			a {
				.marker {
					height: 2px;
					bottom: -2px;
				}
			}
		}

	}

	.stage {

		padding-top: 134px;

		.chosen , .ghost {
			margin-bottom: 12px;
			h3 {
				padding: 8px;
				font-size: 42px;
				padding-bottom: 8px;
				line-height: 40px;

			}

			.description {
				padding: 6px 10px;
			}


			.tags {
				padding-left: 10px;
				margin-bottom: 16px;
				margin-top: 0px;
			}

			.tags li {
				display: inline-block;
				font-size: 14px;
				padding-right: 2px;
				margin-right: 8px;
				margin-bottom: 0;

			}

			address {
				
				margin-top: 4px;
			}
		}
	}
}

@media screen and ( max-width: 580px ) {
	header h1  {
		font-size: 26px;
	}
}


@media screen and ( max-width: 480px ) {
	header h1 {
		font-size: 18px;
		background-size: 18px 13px;
		padding-left: 36px;
		//background-position: 4px 8px;
	}

	header h2 {
		font-size: 14px;
	}

	nav {
		font-size: 20px;
		li {

		}

	}

	.stage {
		padding-top: 116px;
	}
